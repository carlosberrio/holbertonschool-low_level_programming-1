#include "holberton.h"
#include <stdio.h>

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
int main(void)
{
	char s1[98] = "Hello ";
	char s2[] = "World!\n";
	char *p;

	printf("\E[42m%s\n", s1);
	printf("\E[31m%s", s2);
	p = _strcat(s1, s2);
	printf("\E[35m%s", s1);
	printf("\E[36m%s", s2);
	printf("\E[34m%s\033[0m", p);
	return (0);
}
